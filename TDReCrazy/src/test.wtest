import ObjetosBase.*
import wollok.game.*
test "testX" {

	const c1 = new Camino(posicion = game.at(0,0))
	const c2 = new Camino(posicion = game.at(0,1))
	const c3 = new Camino(posicion = game.at(0,2))
	const c4 = new Camino(posicion = game.at(1,2))
	const c5 = new Camino(posicion = game.at(1,3))
	const c6 = new Camino(posicion = game.at(1,4))
	const c7 = new Camino(posicion = game.at(2,4))
	const torre = new Torre(posicion = game.at(2,1))
	torre.setPathInRange()
	assert.equals([[c2], [c4]], torre.pathInRange())

}